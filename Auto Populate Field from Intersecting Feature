// ðŸŸ¢ Ø¬Ù„Ø¨ Ø·Ø¨Ù‚Ø© Ù†Ù‚Ø§Ø· Ø§Ù„ØªÙˆØ²ÙŠØ¹ Ù…Ø¹ Ø§Ù„Ø­Ù‚Ù„ SUBNUMBER
var DBpoint = FeatureSetByName($datastore, "FCL_DISTRIBUTIONBOX", ["SUBNUMBER"]);

// ðŸŸ¢ Ø§Ù„Ø¨Ø­Ø« Ø¹Ù† ØªÙ‚Ø§Ø·Ø¹ Ø¨ÙŠÙ† Ø§Ù„Ù…Ø¹Ù„Ù… Ø§Ù„Ø­Ø§Ù„ÙŠ ÙˆØ§Ù„Ù†Ù‚Ø§Ø·
var DB_intersect = Intersects(DBpoint, Geometry($feature));

// ðŸŸ¢ Ø¥Ø°Ø§ ÙˆÙØ¬Ø¯ ØªÙ‚Ø§Ø·Ø¹ØŒ Ø®Ø° Ø£ÙˆÙ„ Ù†Ù‚Ø·Ø© ÙˆØ£Ø±Ø¬Ø¹ SUBNUMBER
if (Count(DB_intersect) > 0) {
    var val = First(DB_intersect);
    return val.SUBNUMBER;
} else {
    return null;
}
